{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "consistent-return": "warn",
        "no-use-before-define": "off",
        "no-underscore-dangle": "off",
        "no-param-reassign": "warn",
        "max-len": "off",
        "no-console": "off"
    }
}
